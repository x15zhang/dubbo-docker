<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans.xsd        http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <dubbo:application name="consumer-app" />

    <dubbo:registry address="N/A" />

    <dubbo:reference id="greetingService" interface="com.example.service.Greetings" url="dubbo://provider:20880" />
    <dubbo:reference id="helloService" group="helloService" interface="com.example.service.Hello" url="dubbo://provider:20880" />
    <dubbo:reference id="hiService" group="hiService" interface="com.example.service.Hello" url="dubbo://provider:20880" />

</beans>
